{
  "name": "esops",
  "version": "2.5.7",
  "description": " Decouple your infrastructure with auto-ignored template generation.",
  "keywords": [],
  "author": "Sartaj<sartaj@sartaj.me>",
  "license": "MIT",
  "homepage": "https://github.com/sartaj/esops#readme",
  "bugs": {
    "url": "https://github.com/sartaj/esops/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sartaj/esops.git"
  },
  "main": "library/interfaces/main.js",
  "bin": "library/interfaces/cli.js",
  "files": [
    "library"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "test:single": "NODE_ENV=test ts-node-dev --respawn ./node_modules/.bin/tape tests/github.spec.ts",
    "debug": "NODE_ENV=test ts-node-dev --inspect --respawn ./node_modules/.bin/tape tests/**/*.spec.ts | tap-nirvana",
    "test": "NODE_ENV=test ts-node  ./node_modules/.bin/tape tests/**/*.spec.ts | tap-nirvana",
    "test:watch": "NODE_ENV=test ts-node-dev --respawn ./node_modules/.bin/tape tests/**/*.spec.ts | tap-nirvana",
    "test:update": "UPDATE_SNAPSHOTS=1 npm run test",
    "test:e2e": "npm run build && NODE_ENV=e2e ts-node ./tests/e2e-runner.spec.ts",
    "lint": "eslint modules/**/*.ts interfaces/**/*.ts side-effects/**/*.ts tests** utilities/**/*.ts core/**/*.ts run/**/*.ts",
    "report": "NODE_ENV=test nyc --require ts-node/register tape **/*.spec.ts -x **/library/** | tap-nirvana",
    "local-esops": "ts-node -O '{\"lib\": [ \"es7\" ]}' ./usage/cli.ts --overwrite",
    "prepare": "npm run local-esops && npm run build"
  },
  "dependencies": {
    "async": "^2.6.2",
    "await-result": "^2.2.1",
    "boxen": "^1.3.0",
    "chalk": "^2.4.1",
    "cross-spawn": "^6.0.5",
    "fs-jetpack": "^2.2.0",
    "fs-plus": "^3.0.2",
    "ignore": "^5.0.6",
    "is-directory": "^0.3.1",
    "log-driver": "^1.2.7",
    "loglevel": "^1.6.1",
    "marked": "^0.4.0",
    "marked-terminal": "^3.0.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "multimatch": "^2.1.0",
    "pretty-error": "^2.1.1",
    "promised-pipe": "^1.1.1",
    "prompts": "^2.0.0",
    "ramda": "^0.25.0",
    "read-pkg": "^4.0.1",
    "resolve-pkg": "^1.0.0",
    "update-generated-text-fs": "1.0.0",
    "wrap-ansi": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.9",
    "@types/ramda": "^0.25.43",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "await-spawn": "^2.1.2",
    "colors": "^1.3.3",
    "diff": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.2",
    "riteway": "^4.0.1",
    "shorthash": "0.0.2",
    "tap-nirvana": "^1.1.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "tmp": "0.0.33",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.1.6",
    "typescript-node-stack": "github:sartaj/typescript-node-stack"
  },
  "esops": "node:typescript-node-stack/stack"
}
